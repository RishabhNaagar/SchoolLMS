<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. --></div>
					<div class="social-links">
						<a href=""><i class="fa fa-instagram"></i></a>
						<a href=""><i class="fa fa-pinterest"></i></a>
						<a href=""><i class="fa fa-facebook"></i></a>
						<a href=""><i class="fa fa-twitter"></i></a>
						<a href=""><i class="fa fa-youtube"></i></a>
					</div>
				</div>
			</div>
		</div>
	</footer>
	<!-- Footer section end -->
	
	<!--====== Javascripts & Jquery ======-->
	<script src="dashboardjs/jquery-3.2.1.min.js"></script>
	<script src="dashboardjs/bootstrap.min.js"></script>
	<script src="dashboardjs/jquery.slicknav.min.js"></script>
	<script src="dashboardjs/owl.carousel.min.js"></script>
	<script src="dashboardjs/mixitup.min.js"></script>
	<script src="dashboardjs/main.js"></script>

	<!-- Audio Players js -->
	<script src="dashboardjs/jquery.jplayer.min.js"></script>
	<script src="dashboardjs/wavesurfer.min.js"></script>

	<!-- Audio Players Initialization -->
	<script src="dashboardjs/WaveSurferInit.js"></script>
	<script src="dashboardjs/jplayerInit.js"></script>

	</body>
